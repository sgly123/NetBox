{
  "name": "standard",
  "version": "2.0.0",
  "description": "标准NetBox项目模板 - 模块化结构的完整项目",
  "level": 2,
  "complexity": "medium",
  "build_time": "30s",
  "use_cases": ["小型项目", "团队开发", "产品原型"],
  
  "features": {
    "framework": "modular",
    "networking": "basic",
    "plugins": false,
    "database": false,
    "logging": "structured",
    "testing": "comprehensive",
    "documentation": "complete"
  },
  
  "files": [
    {
      "path": "src/main.cpp",
      "type": "source",
      "required": true,
      "description": "主程序入口"
    },
    {
      "path": "src/core/Application.cpp",
      "type": "source",
      "required": true,
      "description": "应用程序核心"
    },
    {
      "path": "src/network/Connection.cpp",
      "type": "source",
      "required": true,
      "description": "网络连接管理"
    },
    {
      "path": "include/netbox/NetBox.h",
      "type": "header",
      "required": true,
      "description": "框架主头文件"
    },
    {
      "path": "include/netbox/Application.h",
      "type": "header",
      "required": true,
      "description": "应用程序接口"
    },
    {
      "path": "include/netbox/Network.h",
      "type": "header",
      "required": true,
      "description": "网络接口"
    },
    {
      "path": "examples/basic/simple_server.cpp",
      "type": "example",
      "required": true,
      "description": "基础服务器示例"
    },
    {
      "path": "examples/advanced/echo_server.cpp",
      "type": "example",
      "required": true,
      "description": "Echo服务器示例"
    },
    {
      "path": "tests/unit/test_application.cpp",
      "type": "test",
      "required": true,
      "description": "应用程序单元测试"
    },
    {
      "path": "tests/unit/test_network.cpp",
      "type": "test",
      "required": true,
      "description": "网络功能单元测试"
    },
    {
      "path": "tests/integration/test_server.cpp",
      "type": "test",
      "required": true,
      "description": "服务器集成测试"
    },
    {
      "path": "CMakeLists.txt",
      "type": "build",
      "required": true,
      "description": "主构建配置"
    },
    {
      "path": "cmake/NetBoxConfig.cmake",
      "type": "build",
      "required": true,
      "description": "框架配置"
    },
    {
      "path": "docs/API.md",
      "type": "doc",
      "required": true,
      "description": "API文档"
    },
    {
      "path": "docs/DEVELOPMENT.md",
      "type": "doc",
      "required": true,
      "description": "开发指南"
    },
    {
      "path": "README.md",
      "type": "doc",
      "required": true,
      "description": "项目说明"
    }
  ],
  
  "dependencies": {
    "system": ["cmake", "make", "g++", "pkg-config"],
    "libraries": ["pthread"],
    "optional": ["gtest", "benchmark"]
  },
  
  "commands": {
    "build": ["mkdir build", "cd build", "cmake ..", "make -j$(nproc)"],
    "test": ["cd build", "ctest --verbose"],
    "benchmark": ["cd build", "./bin/{{project_name}}_benchmark"],
    "run": ["cd build/bin", "./{{project_name}}"],
    "clean": ["rm -rf build"]
  },
  
  "next_steps": [
    "cd {{project_name}}",
    "mkdir build && cd build",
    "cmake .. -DCMAKE_BUILD_TYPE=Release",
    "make -j$(nproc)",
    "./bin/{{project_name}}"
  ]
}
